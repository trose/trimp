{
  "name": "trimpts",
  "version": "0.1.0",
  "description": "HTTP client with browser impersonation for Node.js",
  "main": "dist/Client.js",
  "types": "dist/Client.d.ts",
  "exports": {
    ".": {
      "types": "./dist/Client.d.ts",
      "import": "./dist/Client.js",
      "require": "./dist/Client.js"
    }
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "http",
    "client",
    "browser",
    "impersonation",
    "fingerprinting",
    "nodejs",
    "typescript"
  ],
  "author": "Taylor Rose",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trose/trimp.git"
  },
  "bugs": {
    "url": "https://github.com/trose/trimp/issues"
  },
  "homepage": "https://github.com/trose/trimp#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "source-map": "^0.7.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "nock": "^13.3.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
}
